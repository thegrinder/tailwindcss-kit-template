    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-postcss": "^2.3.4",
    "prettier": "^2.0.5",
    "tailwindcss": "^1.4.6",
    "semantic-release": "^17.0.8"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "docz": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
