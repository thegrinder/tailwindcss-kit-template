{
  "name": "tailwindcss-kit-template",
  "version": "0.0.0-development",
  "description": "Custom set of tailwindcss utilities and components",
  "keywords": [
    "tailwind",
    "tailwindcss",
    "tailwindcss-plugin"
  ],
  "homepage": "https://github.com/thegrinder/tailwindcss-kit-template",
  "repository": {
    "type": "git",
    "url": "https://github.com/thegrinder/tailwindcss-kit-template.git"
  },
  "main": "index.js",
  "scripts": {
    "build:dev": "node_modules/tailwindcss/lib/cli.js build ./styles.css -c ./tailwind.js -o ./dist/styles.css",
    "commit": "git-cz",
    "docz:dev": "docz dev",
    "docz:build": "docz build",
    "docz:serve": "docz build && docz serve",
    "lint": "eslint './src/**/*.js' --fix",
    "semantic-release": "semantic-release"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "eslint --fix"
    ]
  },
  "author": "Pawel Szczepkowski",
  "license": "MIT",
  "devDependencies": {
    "@semantic-release/commit-analyzer": "^8.0.1",
    "@semantic-release/git": "^9.0.0",
    "@semantic-release/npm": "^7.0.5",
    "@semantic-release/release-notes-generator": "^9.0.1",
    "commitizen": "^4.1.2",
    "cz-conventional-changelog": "^3.2.0",
    "eslint": "7.2.0",
    "eslint-config-airbnb-base": "^14.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.0",
    "gatsby-plugin-postcss": "^2.3.4",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "semantic-release": "^17.0.8",
    "tailwindcss": "^1.4.6"
  },
  "dependencies": {
    "classnames": "^2.2.6",
    "docz": "^2.3.1",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-icons": "^3.10.0",
    "react-modal": "^3.11.2"
  }
}
